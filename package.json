{
  "name": "typescript-course",
  "version": "1.0.0",
  "description": "API desenvolvida utilizando TS, Node, ES6, Postegres e Sequelize",
  "main": "server/server.ts",
  "scripts": {
    "start": "cross-env NODE_ENV=development ./node_modules/.bin/ts-node ./server/server.ts",
    "builded": "cross-env NODE_ENV=development node ./build/server/server.js",
    "unit-test": "cross-env NODE_ENV=test mocha --opts build/test/unit/config/mocha.opts ./build/tests/unit/unit.test.js",
    "integration-test": "cross-env NODE_ENV=test mocha --opts build/tests/integration/config/mocha.opts ./build/tests/integration/integration.test.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "API",
    "TypeScript",
    "Node",
    "ES6"
  ],
  "author": "Robson Stein",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "cross-env": "^5.2.0",
    "express": "^4.16.3",
    "http": "0.0.0",
    "http-status": "^1.2.0",
    "morgan": "^1.9.1",
    "pg": "^7.5.0",
    "sequelize-cli": "^4.1.1",
    "typescript": "^3.1.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.6",
    "@types/express": "^4.16.0",
    "@types/http-status": "^0.2.30",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/sequelize": "^4.27.27",
    "@types/supertest": "^2.0.6",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "sequelize": "^4.39.0",
    "supertest": "^3.3.0",
    "testdouble": "^3.8.2",
    "ts-node": "^7.0.1"
  }
}
